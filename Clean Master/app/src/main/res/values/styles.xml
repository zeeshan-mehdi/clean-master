    <style name="checkBoxStyle" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/gray</item>
        <item name="controlBackground">@color/white</item>
        <item name="colorControlActivated">@color/defaultGreen2</item>
    </style>

    <style name="tallerBarStyle" parent="@android:style/Widget.SeekBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progressbar_style</item>
        <item name="android:indeterminateDrawable">@drawable/horizontal_progressbar</item>
        <item name="android:minHeight">8dip</item>
        <item name="android:maxHeight">25dip</item>

    </style>


    <style name="ProgressBarStyle">
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="brownTheme" parent="Theme.AppCompat.DayNight.DarkActionBar">
